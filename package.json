{
  "name": "bahamutbot-api",
  "version": "1.4.0",
  "description": "An api interface for Bahamut bot.",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build-ts": "rm -rf ./dist/* && tsc",
    "start": "cross-env NODE_NO_WARNINGS=1 node ./dist/index.js",
    "start_update": "cross-env NODE_NO_WARNINGS=1 npm update && node ./dist/index.js",
    "dev": "cross-env NODE_NO_WARNINGS=1 node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js",
    "dev_update": "cross-env NODE_NO_WARNINGS=1 npm install && npm update && node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "license": "MIT",
  "author": "Christian Holzwarth",
  "dependencies": {
    "@twurple/api": "^7.0.1",
    "@twurple/auth": "^7.0.1",
    "@twurple/chat": "^7.0.1",
    "@twurple/eventsub-base": "^7.0.1",
    "@twurple/eventsub-http": "^7.0.1",
    "axios": "1.4.0",
    "bahamutbot": "git+https://github.com/TheExoduser/bahamut-bot-ts.git",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-defend": "^1.0.9",
    "express-rate-limit": "^6.8.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "is-reachable": "^5.2.1",
    "iso-639-1": "^2.1.15",
    "latinize": "^0.5.0",
    "luxon": "^3.3.0",
    "mariadb": "^3.2.0",
    "node-emoji": "^2.1.0",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "parseboolean": "^1.0.0",
    "querystring": "^0.2.0",
    "recursive-readdir": "^2.2.3",
    "sequelize": "^6.32.1",
    "slugify": "^1.6.6",
    "snoowrap": "^1.23.0",
    "url": "^0.11.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/latinize": "^0.2.15",
    "@types/luxon": "^3.3.1",
    "@types/node": "^20.4.5",
    "@types/node-emoji": "^1.8.2",
    "@types/node-os-utils": "^1.3.1",
    "@types/node-schedule": "^2.1.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "8.45.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
