{
  "name": "bahamutbot-api",
  "version": "0.4.0",
  "description": "An api interface for Bahamut bot.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "cross-env NODE_NO_WARNINGS=1 node ./dist/index.js",
    "start_update": "cross-env NODE_NO_WARNINGS=1 npm update && node ./dist/index.js",
    "dev": "cross-env NODE_NO_WARNINGS=1 node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js",
    "dev_update": "cross-env NODE_NO_WARNINGS=1 npm install && npm update && node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "license": "MIT",
  "author": "Christian Holzwarth",
  "dependencies": {
    "@twurple/api": "^5.2.7",
    "@twurple/auth": "^5.2.7",
    "@twurple/chat": "^5.2.7",
    "@twurple/eventsub": "^5.2.7",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-defend": "^1.0.9",
    "express-rate-limit": "^6.7.0",
    "fs-extra": "^10.1.0",
    "is-reachable": "^5.2.1",
    "iso-639-1": "^2.1.15",
    "latinize": "^0.5.0",
    "luxon": "^3.1.0",
    "mariadb": "^3.0.2",
    "node-emoji": "^1.11.0",
    "node-fetch": "^3.3.0",
    "node-schedule": "^2.1.0",
    "parseboolean": "^1.0.0",
    "querystring": "^0.2.0",
    "recursive-readdir": "^2.2.3",
    "sequelize": "^6.25.6",
    "slugify": "^1.6.5",
    "url": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/luxon": "^3.1.0",
    "@types/latinize": "^0.2.15",
    "@types/node": "^18.11.9",
    "@types/node-emoji": "^1.8.2",
    "@types/node-schedule": "^2.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.42.1",
    "bahamutbot": "git+https://github.com/TheExoduser/bahamut-bot-ts.git",
    "cross-env": "^7.0.3",
    "eslint": "8.22.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
